        If Not lblTMDBHeader.Tag Is Nothing Then
            lblTMDBHeader.ForeColor = DirectCast(lblTMDBHeader.Tag, Color)
            Me.Cursor = Cursors.Default
            lblTMDBHeader.Tag = Nothing
        End If
    End Sub

